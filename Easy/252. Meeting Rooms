class Solution:
    def canAttendMeetings(self, intervals: List[List[int]]) -> bool:
        #there shouldn't be any overlapping intervals
        if len(intervals) == 0:
            return True
        intervals = sorted(intervals, key = lambda x:x[0])
        stack = [intervals[0]]
        for idx in range(1,len(intervals)):
            if stack[-1][0] == intervals[idx][0] or stack[-1][1] > intervals[idx][0] or stack[-1][1] >= intervals[idx][1]:
                return False
            stack.append(intervals[idx])
        return True
