class Solution:
    def makeFancyString(self, s: str) -> str:
        i = 0 
        res = ""
        while i < len(s):
            if i+2 < len(s) and s[i] == s[i+1] and s[i+1] == s[i+2]:
               #do nothing
                pass
            else:
                res += s[i]
            i += 1
        return res 
