class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        myset = set()
        result = []
        for i in nums:
            rest = target - i
            if (rest in myset):
                result.append(rest)
                result.append(i)
                first = nums.index(result[0])
                try:
                    second = nums.index(result[0],first+1)
                except ValueError:
                    second = nums.index(result[1])
                return first,second
            myset.add(i)
            
 #time complexity O(n) 
