class Solution:
    def allPathsSourceTarget(self, graph: List[List[int]]) -> List[List[int]]:
        n = len(graph)

        adj_list = {}
        for node in range(n):
            adj_list[node] = graph[node]
        
        stack = [(0, [0])]
        res = []
        while stack:
            node, path = stack.pop()
            if node == n-1:#destination
                res.append(path)
                
            for neighbor in adj_list[node]:
                stack.append((neighbor,path+[neighbor]))
        
        return res
